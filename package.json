{
  "name": "@applejax2/tanukimcp-thought",
  "version": "1.0.0",
  "description": "Sequential Prompting Framework MCP Server: Transform thoughts into structured, executable tasks using the IDE's LLM capabilities",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "tanukimcp-thought": "./bin/tanukimcp-thought.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js --ide-llm",
    "dev": "node --loader ts-node/esm src/index.ts",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:tool-scan": "node scripts/test-tool-scan.js"
  },
  "keywords": [
    "mcp",
    "sequential-thinking",
    "ai",
    "agents",
    "task-management",
    "smithery",
    "cursor",
    "claude"
  ],
  "author": "AppleJax2",
  "license": "MIT",
  "dependencies": {
    "fastmcp": "^1.27.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AppleJax2/tanukimcp-thought.git"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/",
    "bin/",
    "tools-response.json",
    "tools-manifest.json",
    "smithery.yaml",
    "README.md",
    "LICENSE"
  ]
}
